<h2>Categorias</h2>
@{
    if (TempData["pc_nome"] != null)
    {
        <h5>Plano de Categorias: @TempData["pc_nome"].ToString()</h5>
    }
    else
    {
        <h5>Plano de Contas: Falha</h5>
    }
}

@{
    if (TempData["plano_nome"] != null)
    {
        <h6>Plano de Contas: @TempData["plano_nome"].ToString()</h6>
    }
    else
    {
        <h6>Plano de Contas: Falha</h6>
    }
}
<div class="btn-tools">
    <div class="row">
        <div class="col-10" style="text-align:left">
            <a asp-area="Contabilidade" asp-controller="PlanoCategorias" asp-action="Index">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-arrow-left-circle" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                    <path fill-rule="evenodd" d="M8 15A7 7 0 1 0 8 1a7 7 0 0 0 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z" />
                    <path fill-rule="evenodd" d="M12 8a.5.5 0 0 1-.5.5H5.707l2.147 2.146a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 1 1 .708.708L5.707 7.5H11.5a.5.5 0 0 1 .5.5z" />
                </svg>
                <span>Voltar</span>
            </a>
        </div>
        <div class="col-2" style="text-align:right">
            <a onClick="window.print()">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-printer" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                    <path d="M11 2H5a1 1 0 0 0-1 1v2H3V3a2 2 0 0 1 2-2h6a2 2 0 0 1 2 2v2h-1V3a1 1 0 0 0-1-1zm3 4H2a1 1 0 0 0-1 1v3a1 1 0 0 0 1 1h1v1H2a2 2 0 0 1-2-2V7a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v3a2 2 0 0 1-2 2h-1v-1h1a1 1 0 0 0 1-1V7a1 1 0 0 0-1-1z" />
                    <path fill-rule="evenodd" d="M11 9H5a1 1 0 0 0-1 1v3a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1v-3a1 1 0 0 0-1-1zM5 8a2 2 0 0 0-2 2v3a2 2 0 0 0 2 2h6a2 2 0 0 0 2-2v-3a2 2 0 0 0-2-2H5z" />
                    <path d="M3 7.5a.5.5 0 1 1-1 0 .5.5 0 0 1 1 0z" />
                </svg>
            </a>
        </div>
    </div>
</div>
<hr class="hrLine" />
@{
    var planoCategorias_id = "0";
    if (@TempData["pc_id"] != null)
    {
        planoCategorias_id = TempData["pc_id"].ToString();
    }

    var planoContas_id = "0";
    if (@TempData["plano_id"] != null)
    {
        planoContas_id = TempData["plano_id"].ToString();
    }
    <div class="table-responsive">
        <table class="table table-sm">
            <caption>Categorias</caption>
            <thead>
                <tr>
                    <th scope="col" style="text-align:left;">Classificação</th>
                    <th scope="col" style="text-align:left;">Nome</th>
                    <th scope="col">Fiscal</th>
                    <th scope="col">Tributo</th>
                    <th scope="col" style="white-space:nowrap;">Padrão</th>
                    <th scope="col" style="white-space:nowrap;">Conta Contábil</th>
                    <th scope="col" style="white-space:nowrap;">Conta on line</th>
                    <th scope="col" style="text-align:right;">Ações</th>
                </tr>
            </thead>
            @{
                foreach (var item in Model.categorias)
                {
                    <tbody>
                        <tr id="line_@item.categoria_id">
                            @{
                                if (item.categoria_classificacao.Length == 1)
                                {
                                    <td style="white-space:nowrap;text-align:left;font-weight:bold;">@item.categoria_classificacao</td>
                                    <td style="white-space:nowrap;text-align:left;font-weight:bold;">@item.categoria_nome.ToUpper()</td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>

                                }
                                if (item.categoria_classificacao.Length == 4)
                                {
                                    <td style="white-space:nowrap;text-align:left;font-weight:bold;">@item.categoria_classificacao</td>
                                    <td style="white-space:nowrap;text-align:left;font-weight:bold;">@item.categoria_nome</td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>
                                    <td></td>

                                }
                                if (item.categoria_classificacao.Length > 4)
                                {
                                    <td style="white-space:nowrap;text-align:left;">@item.categoria_classificacao</td>
                                    <td style="white-space:nowrap;text-align:left;">@item.categoria_nome</td>

                                    if (item.categoria_categoria_fiscal == true)
                                    {
                                        <td style="white-space:nowrap;">Sim</td>
                                    }
                                    else
                                    {
                                        <td style="white-space:nowrap;">Não</td>
                                    }

                                    if (item.categoria_categoria_tributo == true)
                                    {
                                        <td style="white-space:nowrap;">T</td>
                                    }
                                    else
                                    {
                                        <td style="white-space:nowrap;"></td>
                                    }
                                    <td style="white-space:nowrap;">@item.categoria_padrao</td>
                                    <td style="white-space:nowrap;">@item.categoria_conta_contabil</td>
                                    <td style="white-space:nowrap;">
                                        @{
                                            if (item.categoria_classificacao.Length > 4)
                                            {
                                                if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasCreate)
                                                {
                                                    if ((item.categoria_contaonline == null || item.categoria_contaonline == ""))
                                                    {
                                                        <a class="createCategoria_contaonlinePlano" data-categoria_id="@item.categoria_id" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                            <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-link-45deg" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                                <path d="M4.715 6.542L3.343 7.914a3 3 0 1 0 4.243 4.243l1.828-1.829A3 3 0 0 0 8.586 5.5L8 6.086a1.001 1.001 0 0 0-.154.199 2 2 0 0 1 .861 3.337L6.88 11.45a2 2 0 1 1-2.83-2.83l.793-.792a4.018 4.018 0 0 1-.128-1.287z" />
                                                                <path d="M6.586 4.672A3 3 0 0 0 7.414 9.5l.775-.776a2 2 0 0 1-.896-3.346L9.12 3.55a2 2 0 0 1 2.83 2.83l-.793.792c.112.42.155.855.128 1.287l1.372-1.372a3 3 0 0 0-4.243-4.243L6.586 4.672z" />
                                                            </svg>
                                                        </a>
                                                    }
                                                    else
                                                    {
                                                        <a class="DetailsCCOPlano" data-id="@item.categoria_contaonline_id" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                            <span>@item.categoria_contaonline</span>
                                                        </a>
                                                    }
                                                }
                                            }
                                        }
                                    </td>

                                }
                                <td style="white-space:nowrap;text-align:right">
                                    @{
                                        if (item.categoria_classificacao.Length == 1)
                                        {
                                            if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasCreate)
                                            {
                                                if (item.categoria_classificacao.Equals("1"))
                                                {
                                                    <a class="createGrupoCategoriaPlano" data-escopo="Entrada" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-plus-circle" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                            <path fill-rule="evenodd" d="M8 15A7 7 0 1 0 8 1a7 7 0 0 0 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z" />
                                                            <path fill-rule="evenodd" d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
                                                        </svg>
                                                    </a>
                                                }

                                                if (item.categoria_classificacao.Equals("2"))
                                                {
                                                    <a class="createGrupoCategoriaPlano" data-escopo="Saída" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-plus-circle" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                            <path fill-rule="evenodd" d="M8 15A7 7 0 1 0 8 1a7 7 0 0 0 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z" />
                                                            <path fill-rule="evenodd" d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
                                                        </svg>
                                                    </a>
                                                }
                                            }
                                        }
                                        if (item.categoria_classificacao.Length == 4)
                                        {
                                            if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasCreate)
                                            {
                                                <a class="createCategoriaPlano" data-grupo="@item.categoria_classificacao" data-escopo="@item.categoria_escopo" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-plus" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                        <path fill-rule="evenodd" d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4z" />
                                                    </svg>
                                                </a>
                                            }
                                            if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasEdit)
                                            {
                                                <a class="editCategoriaPlano" data-id="@item.categoria_id" data-tipo="@item.categoria_tipo" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-pencil" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                        <path fill-rule="evenodd" d="M12.146.146a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-10 10a.5.5 0 0 1-.168.11l-5 2a.5.5 0 0 1-.65-.65l2-5a.5.5 0 0 1 .11-.168l10-10zM11.207 2.5L13.5 4.793 14.793 3.5 12.5 1.207 11.207 2.5zm1.586 3L10.5 3.207 4 9.707V10h.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.5h.293l6.5-6.5zm-9.761 5.175l-.106.106-1.528 3.821 3.821-1.528.106-.106A.5.5 0 0 1 5 12.5V12h-.5a.5.5 0 0 1-.5-.5V11h-.5a.5.5 0 0 1-.468-.325z" />
                                                    </svg>
                                                </a>
                                            }
                                            if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasDelete)
                                            {
                                                <a class="deleteCategoriaPlano" data-id="@item.categoria_id" data-tipo="@item.categoria_tipo" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-trash" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                        <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z" />
                                                        <path fill-rule="evenodd" d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4L4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                                    </svg>
                                                </a>
                                            }
                                        }
                                        if (item.categoria_classificacao.Length > 4)
                                        {
                                            if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasEdit)
                                            {
                                                <a class="editCategoriaPlano" data-id="@item.categoria_id" data-tipo="@item.categoria_tipo" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-pencil" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                        <path fill-rule="evenodd" d="M12.146.146a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-10 10a.5.5 0 0 1-.168.11l-5 2a.5.5 0 0 1-.65-.65l2-5a.5.5 0 0 1 .11-.168l10-10zM11.207 2.5L13.5 4.793 14.793 3.5 12.5 1.207 11.207 2.5zm1.586 3L10.5 3.207 4 9.707V10h.5a.5.5 0 0 1 .5.5v.5h.5a.5.5 0 0 1 .5.5v.5h.293l6.5-6.5zm-9.761 5.175l-.106.106-1.528 3.821 3.821-1.528.106-.106A.5.5 0 0 1 5 12.5V12h-.5a.5.5 0 0 1-.5-.5V11h-.5a.5.5 0 0 1-.468-.325z" />
                                                    </svg>
                                                </a>
                                            }
                                            if (Model.user.Role == "adm" || Model.user._permissoes.planoCategoriasDelete)
                                            {
                                                <a class="deleteCategoriaPlano" data-id="@item.categoria_id" data-tipo="@item.categoria_tipo" data-planoCategorias_id="@planoCategorias_id" data-planoContas_id="@planoContas_id">
                                                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-trash" fill="#ff4400" xmlns="http://www.w3.org/2000/svg">
                                                        <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z" />
                                                        <path fill-rule="evenodd" d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4L4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z" />
                                                    </svg>
                                                </a>
                                            }
                                        }
                                        <!--Dots Verticals opções para definição categoria padrão-->
                                        if (item.categoria_classificacao.Length > 4)
                                        {
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="#ff4400" class="bi bi-three-dots-vertical" viewBox="0 0 16 16" style="cursor:pointer;" id="@item.categoria_id" onclick="box_opc_dots(this.id, event)" onscroll="">
                                                <path d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0z" />
                                            </svg>
                                        }
                                        else
                                        {
                                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="white" class="bi bi-three-dots-vertical" viewBox="0 0 16 16">
                                                <path d="M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0z" />
                                            </svg>
                                        }
                                    }
                                </td>
                            }
                        </tr>
                    </tbody>
                }
            }
        </table>
