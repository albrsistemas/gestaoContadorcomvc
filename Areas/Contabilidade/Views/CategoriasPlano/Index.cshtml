@model gestaoContadorcomvc.Models.ViewModel.Vm_categoria

@{
    ViewData["Title"] = "Categorias";
    Layout = "~/Areas/Contabilidade/Views/Shared/_Layout.cshtml";
}

<style>
    .col-list-rigth {
        width: 90px;
        text-align: right;
    }

    .btn-categoria {
        float: right;
        margin-top: 10px;
        margin-bottom: 5px;
    }

    .check-list-label {
        display: none;
    }

    .col-list-body {
        width: calc(100% - 90px);
    }

    a {
        cursor: pointer;
    }

    .line-list {
        border: 0px;
    }

    .table > tbody > tr > td {
        vertical-align: middle;
    }

    .table > tbody > tr > th {
        vertical-align: middle;
    }

    .table > tbody > tr:hover {
        background-color: #f5f4ba;
    }

    .box_opc_dots_glass {
        width: 100%;
        height: 100%;
        position: fixed;
        top: 92px;
        left: 0px;
    }

    .box_opc_dots {
        min-width: 150px;
        height: auto;
        background-color: rgb(255,230,153);
        position: fixed;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        box-shadow: -1px 0px 5px #6b5858;
        text-align: center;
    }

    .box_opc_dots_title {
        min-width: 150px;
        height: 30px;
        line-height: 30px;
        border-top-left-radius: 5px;
        border-top-right-radius: 5px;
        border-bottom: 1px solid orange;
    }

    .box_opc_dots_itens {
        min-width: 150px;
        height: 30px;
        line-height: 30px;
        cursor: pointer;
        text-align: left;
        padding-left: 5px;
        padding-right: 10px;
    }

        .box_opc_dots_itens:hover {
            background-color: rgb(255,69,0);
            color: white;
        }

    .container {
        min-width: 100%;
    }
</style>

